{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "38": "images/icon-38.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/libs/underscore-min.js",
      "scripts/libs/MockStorage.js",
      "scripts/libs/ActivityLog.js",
      "scripts/background.js"
    ]
  },
  "permissions": ["<all_urls>", "storage", "unlimitedStorage"],
  "web_accessible_resources": [
    "scripts/libs/MockHttpRequest.js",
    "scripts/libs/CustomEventServer.js",
    "scripts/injected.js"
  ],
  "browser_action": {
    "default_icon": "images/icon-38.png",
    "default_title": "Start learning"
  },
  "options_page": "options_page/dist/index.html"
}